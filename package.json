{
  "name": "heapify",
  "version": "0.5.0",
  "description": "Blazing fast JavaScript priority queue",
  "repository": {
    "type": "git",
    "url": "git://github.com/luciopaiva/heapify.git"
  },
  "main": "dist/heapify.node.js",
  "types": "dist/heapify.d.ts",
  "scripts": {
    "test": "c8 mocha --recursive",
    "report-coverage": "c8 report --reporter=text-lcov > coverage.lcov",
    "count": "cloc src/heapify.ts",
    "bench": "node benchmark/index.mjs",
    "linter": "eslint . --ext .ts test/**/*.mjs benchmark/**/*.mjs",
    "build": "webpack --mode=production --node-env=production",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production --node-env=production",
    "watch": "webpack --watch"
  },
  "keywords": [
    "priority queue",
    "queue",
    "heap",
    "minheap",
    "maxheap",
    "binary heap"
  ],
  "author": {
    "name": "Lucio Paiva",
    "url": "https://luciopaiva.com"
  },
  "homepage": "https://github.com/luciopaiva/heapify",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "c8": "^7.3.0",
    "cloc": "^2.5.1",
    "codecov": "^3.6.5",
    "eslint": "^8.5.0",
    "fastpriorityqueue": "0.6.3",
    "flatqueue": "1.1.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "google-closure-library": "20200830.0.0",
    "mocha": "^9.1.3",
    "tinyqueue": "2.0.3",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.4",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1"
  }
}
